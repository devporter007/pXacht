{
    "name": "pXacht Chrome Helper",
    "description": "Supplies all required information to kXacht.",
    "version": "0.1",
    "background": {
        "service_worker": "service_worker.js"
      },
    "manifest_version": 3,
    "minimum_chrome_version": "46",
    "permissions":[
        "tabs","storage","webRequest","activeTab"
    ],
	"host_permissions":[
		"*://*/*"
	],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
   
  }